[
  {
    "selector": "node.network",
    "style": {
      "label": "data(title)",
      "width": 24,
      "height": 24,
      "border-width": 1,
      "border-style": "solid",
      "border-color": "#6ab9ff",
      "background-color": "#e1f1ff",
      "color": "#ffffff",
      "font-weight": "normal",
      "text-halign": "center",
      "text-valign": "center",
      "text-outline-color": "#6ab9ff",
      "text-outline-width": 1
    }
  },
  {
    "selector": "node.network.device",
    "style": {
      "shape": "round-rectangle",
      "width": 48,
      "height": 48
    }
  },
  {
    "selector": "edge.network",
    "style": {
      "width": 1,
      "line-color": "#6ab9ff",
      "curve-style": "bezier",
      "target-arrow-color": "#6ab9ff"
    }
  },
  {
    "selector": "edge.network.access",
    "style": {
      "target-arrow-shape": "vee",
      "target-arrow-fill": "filled"
    }
  },
  {
    "selector": "edge.network.connection",
    "style": {
      "target-arrow-shape": "diamond",
      "target-arrow-fill": "hollow"
    }
  }
]