[
  {
    "selector": ".network",
    "style": {
      "border-color": "#6ab9ff",
      "line-color": "#6ab9ff",
      "target-arrow-color": "#6ab9ff",
      "background-color": "#e1f1ff"
    }
  },
  {
    "selector": "node.resource",
    "style": {
      "label": "data(name)",
      "width": 48,
      "height": 48,
      "border-width": 1,
      "border-style": "solid",
      "shape": "round-hexagon",
      "background-fit": "none",
      "background-repeat": "no-repeat",
      "background-width": "50%",
      "background-height": "50%",
      "background-clip": "node"
    }
  },
  {
    "selector": "node.interface",
    "style": {
      "label": "data(name)",
      "shape": "ellipse",
      "width": 16,
      "height": 16,
      "border-width": 2,
      "border-style": "solid",
      "background-color": "#fff"
    }
  },
  {
    "selector": "node#internet",
    "style": {
      "background-image": "/public/images/001-cloud.png"
    }
  },
  {
    "selector": "node#intranet",
    "style": {
      "background-image": "/public/images/002-network.png"
    }
  },
  {
    "selector": "edge.use",
    "style": {
      "curve-style": "bezier",
      "target-arrow-shape": "tee",
      "width": 1
    }
  },
  {
    "selector": "edge.provide",
    "style": {
      "curve-style": "bezier",
      "width": 1
    }
  }
]